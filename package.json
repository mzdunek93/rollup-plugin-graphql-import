{
  "name": "rollup-plugin-graphql-import",
  "version": "1.1.0",
  "description": "Rollup plugin to import graphql schema definitions",
  "author": "Dennis Torres <djtorres0@gmail.com>",
  "bugs": "https://github.com/mzdunek93/rollup-plugin-graphql-import/issues",
  "dependencies": {
    "@graphql-toolkit/code-file-loader": "^0.10.2",
    "@graphql-toolkit/core": "^0.10.2",
    "@graphql-toolkit/graphql-file-loader": "^0.10.2",
    "@graphql-toolkit/json-file-loader": "^0.10.2",
    "@graphql-toolkit/prisma-loader": "^0.10.2",
    "@graphql-toolkit/url-loader": "^0.10.2",
    "find-up": "^4.1.0",
    "graphql": "^15.0.0",
    "magic-string": "^0.25.7",
    "rollup-pluginutils": "^2.8.2"
  },
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@psirenny/remark-preset": "4.0.0",
    "@psirenny/retext-preset": "4.0.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "ava": "3.6.0",
    "dictionary-en": "3.0.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-ava": "10.2.0",
    "eslint-plugin-markdown": "1.0.2",
    "eslint-plugin-prettier": "^3.1.2",
    "graphql-schema-linter": "0.2.5",
    "husky": "4.2.3",
    "lint-staged": ">=10",
    "npm-run-all": "4.1.5",
    "nyc": "15.0.1",
    "prettier": "^2.0.4",
    "remark-cli": "8.0.0",
    "remark-lint-blockquote-indentation": "2.0.0",
    "remark-lint-checkbox-character-style": "2.0.0",
    "remark-lint-code-block-style": "2.0.0",
    "remark-lint-emphasis-marker": "2.0.0",
    "remark-lint-fenced-code-marker": "2.0.0",
    "remark-lint-heading-style": "2.0.0",
    "remark-lint-link-title-style": "2.0.0",
    "remark-lint-rule-style": "2.0.0",
    "remark-lint-strong-marker": "2.0.0",
    "remark-lint-table-cell-padding": "2.0.0",
    "remark-preset-lint-consistent": "3.0.0",
    "remark-preset-lint-markdown-style-guide": "3.0.0",
    "remark-preset-lint-recommended": "4.0.0",
    "remark-retext": "4.0.0",
    "retext-assuming": "1.0.0",
    "retext-contractions": "4.0.0",
    "retext-diacritics": "3.0.0",
    "retext-english": "3.0.4",
    "retext-equality": "5.0.0",
    "retext-indefinite-article": "2.0.0",
    "retext-intensify": "5.0.0",
    "retext-passive": "3.0.0",
    "retext-profanities": "6.0.0",
    "retext-quotes": "4.0.0",
    "retext-readability": "6.0.0",
    "retext-redundant-acronyms": "3.0.0",
    "retext-repeated-words": "3.0.0",
    "retext-sentence-spacing": "4.0.0",
    "retext-simplify": "6.0.0",
    "retext-spell": "4.0.0",
    "retext-syntax-mentions": "2.0.0",
    "retext-syntax-urls": "2.0.0",
    "rollup": "2.3.3",
    "rollup-plugin-typescript": "^1.0.1",
    "standard-version": "7.1.0",
    "trash-cli": "3.0.0",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "unified": "9.0.0"
  },
  "homepage": "https://github.com/mzdunek93/rollup-plugin-graphql-import",
  "keywords": [
    "graphql",
    "import",
    "rollup-plugin",
    "schema"
  ],
  "license": "Unlicense",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "repository": "github:mzdunek93/rollup-plugin-graphql-import",
  "scripts": {
    "audit": "yarn audit",
    "build": "rollup -c",
    "check": "run-s audit deps:check lint test test:cov typecheck",
    "ci:check": "circleci local execute --job check",
    "clean": "trash **.DS_Store *.log .bin .nyc_* dist node_modules",
    "deps:check": "run-s deps:check:verify deps:check:integrity",
    "deps:check:integrity": "yarn check --integrity",
    "deps:check:verify": "yarn check --verify-tree",
    "lint": "run-s lint:graphql lint:md lint:ts",
    "lint:graphql": "graphql-schema-linter test/**/*.graphql",
    "lint:md": "remark --frail .",
    "lint:ts": "eslint . --ext .ts,.md",
    "outdated": "yarn outdated",
    "prerelease": "run-s build",
    "release": "standard-version",
    "test": "nyc ava --verbose",
    "test:cov": "nyc check-coverage",
    "test:cov:report": "nyc report --reporter=html",
    "update": "run-s update:js update:yarn",
    "update:js": "yarn upgrade-interactive --latest",
    "update:yarn": "yarn policies set-version"
  },
  "sideEffects": false,
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.md": "remark --frail",
    "*.graphql": "graphql-schema-linter",
    "*.{ts,md}": "eslint --cache --fix"
  }
}
